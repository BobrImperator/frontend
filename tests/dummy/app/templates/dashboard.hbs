<CommonDashboard
  @allSchools={{this.model.schools}}
  @allAcademicYears={{this.model.academicYears}}
  @show={{this.show}}
  @setShow={{fn (mut this.show)}}
  @selectedDate={{this.selectedDate}}
  @selectedView={{this.selectedView}}
  @mySchedule={{this.mySchedule}}
  @showFilters={{this.showFilters}}
  @changeDate={{fn this.changeDate}}
  @changeView={{fn (mut this.view)}}
  @selectEvent={{fn this.selectEvent}}
  @toggleMySchedule={{fn this.toggleMySchedule}}
  @toggleShowFilters={{fn this.toggleShowFilters}}
  @selectedAcademicYear={{this.selectedAcademicYear}}
  @selectedSchool={{this.selectedSchool}}
  @changeAcademicYear={{fn (mut this.academicYear)}}
  @changeSchool={{fn (mut this.school)}}
  @courseFilters={{this.courseFilters}}
  @toggleCourseFilters={{fn (set this.courseFilters (not this.courseFilters))}}
  @selectedCohorts={{await this.selectedCohorts}}
  @selectedCourseLevels={{this.selectedCourseLevels}}
  @selectedCourses={{await this.selectedCourses}}
  @selectedSessionTypes={{await this.selectedSessionTypes}}
  @selectedTerms={{await this.selectedTerms}}
  @clearFilters={{fn this.clearFilters}}
  @updateCohorts={{fn this.updateCohorts}}
  @updateCourseLevels={{fn this.updateCourseLevels}}
  @updateCourses={{fn this.updateCourses}}
  @updateSessionTypes={{fn this.updateSessionTypes}}
  @updateTerms={{fn this.updateTerms}}
/>
