<DashboardViewPicker @show={{this.show}} @change={{this.setShow}} />
{{#if (eq this.show "calendar")}}
  <DashboardCalendar
    @selectedDate={{this.selectedDate}}
    @selectedView={{this.selectedView}}
    @mySchedule={{this.mySchedule}}
    @showFilters={{this.showFilters}}
    @changeDate={{this.changeDate}}
    @changeView={{this.changeView}}
    @selectEvent={{this.selectEvent}}
    @toggleMySchedule={{this.toggleMySchedule}}
    @toggleShowFilters={{this.toggleShowFilters}}
    @selectedAcademicYear={{this.selectedAcademicYear}}
    @selectedSchool={{this.selectedSchool}}
    @changeAcademicYear={{this.changeAcademicYear}}
    @changeSchool={{this.changeSchool}}
    @courseFilters={{this.courseFilters}}
    @toggleCourseFilters={{this.toggleCourseFilters}}
    @selectedCohorts={{this.selectedCohorts}}
    @selectedCourseLevels={{this.selectedCourseLevels}}
    @selectedCourses={{this.selectedCourses}}
    @selectedSessionTypes={{this.selectedSessionTypes}}
    @selectedTerms={{this.selectedTerms}}
    @onClearFilters={{this.onClearFilters}}
    @onUpdateCohorts={{this.onUpdateCohorts}}
    @onUpdateCourseLevels={{this.onUpdateCourseLevels}}
    @onUpdateCourses={{this.onUpdateCourses}}
    @onUpdateSessionTypes={{this.onUpdateSessionTypes}}
    @onUpdateTerms={{this.onUpdateTerms}}
  />
{{else if (eq this.show "materials")}}
  <DashboardMaterials />
{{else if (eq this.show "agenda")}}
  <DashboardAgenda />
{{else if (eq this.show "week")}}
  <DashboardWeek />
{{/if}}
