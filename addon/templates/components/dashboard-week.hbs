<span class='weeklylink'>
  {{t 'general.view'}}:
  {{#link-to
    'weeklyevents'
    (query-params expanded=expanded)
  }}
    {{t 'general.lastNextAllWeeks'}}
  {{/link-to}}
</span>
{{week-glance
  collapsible=false
  collapsed=false
  showFullTitle=true
  year=(moment-format today 'YYYY')
  week=(moment-format today 'W')
}}
