<div class="title">
  {{t "general.missingItems"}} ({{this.course.allPublicationIssuesLength}})
</div>
<div class="course-publicationcheck-content">
  <table>
    <thead>
      <tr>
        <th>
          {{t "general.courseTitle"}}
        </th>
        <th>
          {{t "general.cohorts"}}
        </th>
        <th>
          {{t "general.terms"}}
        </th>
        <th>
          {{t "general.objectives"}}
        </th>
        <th>
          {{t "general.meshTerms"}}
        </th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>
          {{this.course.title}}
        </td>
        {{#if this.course.cohorts.length}}
          <td class="yes">
            {{t "general.yes"}} ({{this.course.cohorts.length}})
          </td>
        {{else}}
          <td class="no">
            {{t "general.no"}}
          </td>
        {{/if}}
        {{#if this.course.terms.length}}
          <td class="yes">
            {{t "general.yes"}} ({{this.course.terms.length}})
          </td>
        {{else}}
          <td class="no">
            {{t "general.no"}}
          </td>
        {{/if}}
        {{#if this.course.objectives.length}}
          <td class="yes">
            {{t "general.yes"}} ({{this.course.objectives.length}})
            {{#if this.showUnlinkIcon}}
              <FaIcon
                @icon="unlink"
                class="clickable"
                @click={{action "transitionToCourse"}}
              />
            {{/if}}
          </td>
        {{else}}
          <td class="no">
            {{t "general.no"}}
          </td>
        {{/if}}
        {{#if this.course.meshDescriptors.length}}
          <td class="yes">
            {{t "general.yes"}} ({{this.course.meshDescriptors.length}})
          </td>
        {{else}}
          <td class="no">
            {{t "general.no"}}
          </td>
        {{/if}}
      </tr>
    </tbody>
  </table>
</div>

