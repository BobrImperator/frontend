<table>
  <caption>{{t "general.table1ProgramExpectationsMappedToPcrs"}}</caption>
  <thead>
    <tr>
      <th>{{t "general.programExpectationsId"}}</th>
      <th colspan="3">{{t "general.programExpectations"}}</th>
      <th colspan="2">{{t "general.physicianCompetencyReferenceSet"}} ({{t "general.physicianCompetencyReferenceSetAbbr"}})</th>
    </tr>
  </thead>
  <tbody>
    {{#each (await data) as |row|}}
      <tr>
        <td>{{t "general.notApplicableAbbr"}}</td>
        <td colspan="3">{{{row.title}}}</td>
        <td colspan="2">
          <ul>
            {{#each row.pcrs as |pcrs|}}
              <li>{{pcrs}}</li>
            {{/each}}
          </ul>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
