<CurriculumInventory::ReportDetails
  @report={{@model}}
  @canUpdate={{this.canUpdate}}
  @leadershipDetails={{this.leadershipDetails}}
  @setLeadershipDetails={{this.setLeadershipDetails}}
  @manageLeadership={{this.manageLeadership}}
  @setManageLeadership={{this.setManageLeadership}}
  @markReportAsFinalized={{set this.isFinalized true}}
/>
<CurriculumInventory::SequenceBlockList
  @report={{@model}}
  @sequenceBlocks={{this.topLevelSequenceBlocks}}
  @canUpdate={{this.canUpdate}}
  @remove={{this.removeSequenceBlock}}
/>
