<SessionOverview
  @course={{course}}
  @session={{this.session}}
  @sessionTypes={{this.sessionTypes}}
  @editable={{this.editable}}
/>
{{#if this.sessionLeadershipDetails}}
  <SessionLeadershipExpanded
    @session={{this.session}}
    @canUpdate={{this.editable}}
    @collapse={{action this.setSessionLeadershipDetails false}}
    @expand={{action this.setSessionLeadershipDetails true}}
    @isManaging={{this.sessionManageLeadership}}
    @setIsManaging={{this.setSessionManageLeadership}}
  />
{{else}}
  <LeadershipCollapsed
    @title={{t "general.sessionAdministration"}}
    @showDirectors={{false}}
    @administratorsCount={{has-many-length this.session "administrators"}}
    @expand={{action this.setSessionLeadershipDetails true}}
  />
{{/if}}
{{#if this.session.isIndependentLearning}}
  {{#if
    (or
      (eq (get (await this.session.ilmSession.learnerGroups) "length") 0)
      this.sessionLearnergroupDetails
    )
  }}
    <DetailLearnergroups
      @subject={{await this.session.ilmSession}}
      @cohorts={{await this.session.course.cohorts}}
      @isIlmSession={{true}}
      @editable={{this.editable}}
      @isManaging={{this.isManagingLearnerGroups}}
      @setIsManaging={{action this.setIsManagingLearnerGroups}}
      @collapse={{action this.setSessionLearnergroupDetails false}}
      @expand={{action this.setSessionLearnergroupDetails true}}
    />
  {{else}}
    <CollapsedLearnergroups
      @subject={{await this.session.ilmSession}}
      @expand={{action this.setSessionLearnergroupDetails true}}
    />
  {{/if}}
  <DetailInstructors
    @ilmSession={{await this.session.ilmSession}}
    @editable={{this.editable}}
  />
{{/if}}
{{#if (or (eq this.session.objectives.length 0) this.sessionObjectiveDetails)}}
  <DetailObjectives
    @subject={{this.session}}
    @isSession={{true}}
    @editable={{this.editable}}
    @collapse={{action this.setSessionObjectiveDetails false}}
    @expand={{action this.setSessionObjectiveDetails true}}
  />
{{else}}
  <CollapsedObjectives
    @subject={{this.session}}
    @editable={{this.editable}}
    @expand={{action this.setSessionObjectiveDetails true}}
  />
{{/if}}
<DetailLearningMaterials
  @subject={{this.session}}
  @isCourse={{false}}
  @editable={{this.editable}}
/>
{{#if (or (eq this.session.terms.length 0) this.sessionTaxonomyDetails)}}
  <DetailTaxonomies
    @subject={{this.session}}
    @editable={{this.editable}}
    @collapse={{action this.setSessionTaxonomyDetails false}}
    @expand={{action this.setSessionTaxonomyDetails true}}
  />
{{else}}
  <CollapsedTaxonomies
    @subject={{this.session}}
    @expand={{action this.setSessionTaxonomyDetails true}}
  />
{{/if}}
<DetailMesh @subject={{this.session}} @isSession={{true}} @editable={{this.editable}} />
<SessionOfferings
  @session={{this.session}}
  @editable={{this.editable}}
  @showNewOfferingForm={{this.showNewOfferingForm}}
  @toggleShowNewOfferingForm={{this.toggleShowNewOfferingForm}}
/>
