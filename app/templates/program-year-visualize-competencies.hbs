<section class="program-year-visualize-competencies">
  <div class="backtolink">
    {{#link-to "program-year-visualizations" model}}{{t "general.otherVisualizations"}}{{/link-to}}
  </div>
  <h3>
    {{#link-to "program" model.program}}
      {{get (await model.program) "title"}}
    {{/link-to}}
    {{#link-to "programYear" model.program model}}
      {{#if (get (await model.cohort) "title")}}
        {{get (await model.cohort) "title"}}
      {{else}}
        {{t "general.classOf" year=(await model.classOfYear)}}
      {{/if}}
    {{/link-to}}
  </h3>
  {{visualizer-program-year-competencies programYear=model}}
</section>
