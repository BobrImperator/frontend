<ProgramYear::Details
  @program={{await @model.program}}
  @canUpdate={{this.canUpdate}}
  @programYear={{@model}}
  @pyObjectiveDetails={{this.pyObjectiveDetails}}
  @pyTaxonomyDetails={{this.pyTaxonomyDetails}}
  @pyCompetencyDetails={{this.pyCompetencyDetails}}
  @managePyCompetencies={{this.managePyCompetencies}}
  @setPyObjectiveDetails={{set this.pyObjectiveDetails}}
  @setPyTaxonomyDetails={{set this.pyTaxonomyDetails}}
  @setPyCompetencyDetails={{set this.pyCompetencyDetails}}
  @setManagePyCompetencies={{set this.managePyCompetencies}}
/>
