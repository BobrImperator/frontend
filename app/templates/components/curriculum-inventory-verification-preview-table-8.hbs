<h4 id="verification-preview-table-8">{{t "general.table8AllResourceTypes"}}</h4>
<table>
  <thead>
    <tr>
      <th>{{t "general.itemCode"}}</th>
      <th colspan="2">{{t "general.resourceTypes"}}</th>
      <th>{{t "general.numberOfEvents"}}</th>
    </tr>
  </thead>
  <tbody>
    {{#each (await @data) as |row|}}
      <tr>
        <td>{{row.id}}</td>
        <td colspan="2">{{row.title}}</td>
        <td>{{row.count}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
{{#if @tocId}}
  <a href="#{{@tocId}}">{{t "general.backToTableOfContents"}}</a>
{{/if}}