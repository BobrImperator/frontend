{{#if
  (or
    (and (eq course.directors.length 0) (eq course.administrators.length 0))
    this.courseLeadershipDetails
  )
}}
  <CourseLeadershipExpanded
    @course={{@course}}
    @editable={{this.editable}}
    @collapse={{action this.setCourseLeadershipDetails false}}
    @expand={{action this.setCourseLeadershipDetails true}}
    @isManaging={{this.courseManageLeadership}}
    @setIsManaging={{this.setCourseManageLeadership}}
  />
{{else}}
  <LeadershipCollapsed
    @title={{t "general.courseLeadership"}}
    @directorsCount={{has-many-length course "directors"}}
    @administratorsCount={{has-many-length course "administrators"}}
    @expand={{action this.setCourseLeadershipDetails true}}
  />
{{/if}}
{{#if (or (eq course.objectives.length 0) this.courseObjectiveDetails)}}
  <DetailObjectives
    @subject={{@course}}
    @isCourse={{true}}
    @editable={{this.editable}}
    @collapse={{action this.setCourseObjectiveDetails false}}
    @expand={{action this.setCourseObjectiveDetails true}}
  />
{{else}}
  <CollapsedObjectives
    @subject={{@course}}
    @expand={{action this.setCourseObjectiveDetails true}}
  />
{{/if}}
<DetailLearningMaterials
  @subject={{@course}}
  @isCourse={{true}}
  @editable={{this.editable}}
/>
{{#if
  (or (eq (get (await course.competencies) "length") 0) this.courseCompetencyDetails)
}}
  <DetailCompetencies
    @course={{@course}}
    @editable={{this.editable}}
    @collapse={{action this.setCourseCompetencyDetails false}}
    @expand={{action this.setCourseCompetencyDetails true}}
  />
{{else}}
  <CollapsedCompetencies
    @subject={{@course}}
    @expand={{action this.setCourseCompetencyDetails true}}
  />
{{/if}}
{{#if (or (eq course.terms.length 0) this.courseTaxonomyDetails)}}
  <DetailTaxonomies
    @subject={{@course}}
    @isCourse={{true}}
    @editable={{this.editable}}
    @collapse={{action this.setCourseTaxonomyDetails false}}
    @expand={{action this.setCourseTaxonomyDetails true}}
  />
{{else}}
  <CollapsedTaxonomies
    @subject={{@course}}
    @expand={{action this.setCourseTaxonomyDetails true}}
  />
{{/if}}
<DetailMesh @subject={{@course}} @isCourse={{true}} @editable={{this.editable}} />
<DetailCohorts @course={{@course}} @editable={{this.editable}} />
