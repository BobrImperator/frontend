{{#if (and startDate endDate)}}
  {{t 'general.timedReleaseStartAndEndDate' startDate=(moment-format startDate 'L LT')  endDate=(moment-format endDate 'L LT')}}
{{/if}}
{{#if (and startDate (not endDate))}}
  {{t 'general.timedReleaseOnlyStartDate' startDate=(moment-format startDate 'L LT')}}
{{/if}}
{{#if (and endDate (not startDate))}}
  {{t 'general.timedReleaseOnlyEndDate' endDate=(moment-format endDate 'L LT')}}
{{/if}}
{{#if (and (not startDate) (not endDate))}}
  {{t 'general.timedReleaseNoSchedule'}}
{{/if}}
