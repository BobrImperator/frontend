<section class="course-visualize-term">
  <div class="backtolink">
    <LinkTo @route="course-visualizations" @model={{this.model.course}}>
      {{t "general.otherVisualizations"}}
    </LinkTo>
  </div>
  <h2>
    {{t
      "general.sessionTypesFor"
      subject=(concat this.model.term.vocabulary.title " - " this.model.term.title)
    }}
  </h2>
  <h3 class="clickable">
    <LinkTo @route="course" @model={{this.model.course}}>
      {{this.model.course.title}}
      {{this.model.course.academicYear}}
    </LinkTo>
  </h3>
  <VisualizerCourseTerm @course={{this.model.course}} @term={{this.model.term}} />
</section>
