{{common-dashboard
  show=this.show
  setShow=(action (mut this.show))
  selectedDate=this.selectedDate
  selectedView=this.selectedView
  mySchedule=this.mySchedule
  showFilters=this.showFilters
  changeDate=(action "changeDate")
  changeView=(action (mut this.view))
  selectEvent=(action "selectEvent")
  toggleMySchedule=(action "toggleMySchedule")
  toggleShowFilters=(action "toggleShowFilters")
  selectedAcademicYear=this.selectedAcademicYear
  selectedSchool=this.selectedSchool
  changeAcademicYear=(action (mut this.academicYear))
  changeSchool=(action (mut this.school))
  courseFilters=this.courseFilters
  toggleCourseFilters=(action (toggle "courseFilters" this))
  selectedCohorts=this.selectedCohorts
  selectedCourseLevels=this.selectedCourseLevels
  selectedCourses=this.selectedCourses
  selectedSessionTypes=this.selectedSessionTypes
  selectedTerms=this.selectedTerms
  onClearFilters=(action "clearFilters")
  onUpdateCohorts=(action "updateCohorts")
  onUpdateCourseLevels=(action "updateCourseLevels")
  onUpdateCourses=(action "updateCourses")
  onUpdateSessionTypes=(action "updateSessionTypes")
  onUpdateTerms=(action "updateTerms")}}
