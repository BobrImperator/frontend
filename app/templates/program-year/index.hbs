<ProgramYear::Details
  @program={{await @model.program}}
  @canUpdate={{this.canUpdate}}
  @programYear={{@model}}
  @pyObjectiveDetails={{this.pyObjectiveDetails}}
  @pyTaxonomyDetails={{this.pyTaxonomyDetails}}
  @pyCompetencyDetails={{this.pyCompetencyDetails}}
  @managePyCompetencies={{this.managePyCompetencies}}
  @setPyObjectiveDetails={{action (mut this.pyObjectiveDetails)}}
  @setPyTaxonomyDetails={{action (mut this.pyTaxonomyDetails)}}
  @setPyCompetencyDetails={{action (mut this.pyCompetencyDetails)}}
  @setManagePyCompetencies={{action (mut this.managePyCompetencies)}}
/>
