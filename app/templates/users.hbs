{{title (t "general.admin")}}
<LinkTo @route="admin-dashboard">
  {{t "general.backToAdminDashboard"}}
</LinkTo>
<IliosUsers
  @offset={{offset}}
  @setOffset={{action (mut offset)}}
  @limit={{limit}}
  @setLimit={{action (mut limit)}}
  @query={{query}}
  @setQuery={{action (mut query) value="target.value"}}
  @showNewUserForm={{showNewUserForm}}
  @setShowNewUserForm={{action (mut showNewUserForm)}}
  @showBulkNewUserForm={{showBulkNewUserForm}}
  @setShowBulkNewUserForm={{action (mut showBulkNewUserForm)}}
  @searchTerms={{searchTerms}}
  @setSearchTerms={{action (mut searchTerms)}}
  @transitionToUser={{action "transitionToUser"}}
/>
