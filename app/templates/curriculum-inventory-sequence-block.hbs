{{title (t "general.curriculumInventoryReports")}}
<CurriculumInventorySequenceBlockDetails
  @sequenceBlock={{model}}
  @canUpdate={{canUpdate}}
  @sortSessionsBy={{sortSessionsBy}}
  @setSortSessionBy={{action (mut sortSessionsBy)}}
/>
<CurriculumInventorySequenceBlockList
  @report={{await model.report}}
  @parent={{model}}
  @canUpdate={{canUpdate}}
  @sequenceBlocks={{await model.children}}
  @remove={{action "removeChildSequenceBlock"}}
/>
